    <decisions>
        <description>Send the Story&apos;s assigned builder user an email notification if the Test Result&apos;s status has changed to Reported.  Only send the email when the current user isn&apos;t the Assigned user for the Story.</description>
        <name>decSendEmail_TestResultReported</name>
        <label>Send Email (TestResultReported)?</label>
        <locationX>182</locationX>
        <locationY>1598</locationY>
        <defaultConnectorLabel>No</defaultConnectorLabel>
        <rules>
            <name>outSendEmail_TestResultReported</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>varRecDraftTestResult.Status__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Reported</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>getTestResult.Status__c</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue>Reported</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>formulaCurrentUserId</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <elementReference>varRecDraftTestResult.Story__r.Assigned__r.User__c</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>subflowSendEmail_TestResultReported</targetReference>
            </connector>
            <label>Yes (Test Result Reported)</label>
        </rules>
    </decisions>
    <decisions>
        <name>decSendEmail_TestResultRetest</name>
        <label>Send Email (TestResultRetest)?</label>
        <locationX>182</locationX>
        <locationY>1298</locationY>
