        <inputAssignments>
            <field>Release__c</field>
            <value>
                <elementReference>input01_Release.outputSelectedReleaseId</elementReference>
            </value>
        </inputAssignments>
        <inputAssignments>
            <field>Requested_Release__c</field>
            <value>
                <stringValue></stringValue>
            </value>
        </inputAssignments>
        <object>Story__c</object>
    </recordUpdates>
