    <recordUpdates>
        <name>updateStory</name>
        <label>Update Story</label>
        <locationX>314</locationX>
        <locationY>1190</locationY>
        <connector>
            <targetReference>getMetadataComponentChanges</targetReference>
        </connector>
        <faultConnector>
            <targetReference>subflowHandleErrorStoryUpdate</targetReference>
        </faultConnector>
        <filterLogic>and</filterLogic>
        <filters>
            <field>Id</field>
            <operator>EqualTo</operator>
            <value>
                <elementReference>recordId</elementReference>
            </value>
