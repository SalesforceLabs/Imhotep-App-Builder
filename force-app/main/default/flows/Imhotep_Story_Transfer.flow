    <decisions>
        <name>decAnyAvailableReleases</name>
        <label>Any Available Releases?</label>
        <locationX>974</locationX>
        <locationY>566</locationY>
        <defaultConnector>
            <targetReference>s02</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>No Releases to Transfer To</defaultConnectorLabel>
        <rules>
            <name>outOtherReleasesAvailable</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>getReleases</leftValueReference>
                <operator>IsEmpty</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>s01</targetReference>
            </connector>
            <label>Other Releases Available</label>
        </rules>
    </decisions>
    <decisions>
        <name>decAnyChanges</name>
        <label>Any Changes?</label>
        <locationX>314</locationX>
        <locationY>1514</locationY>
        <defaultConnectorLabel>No</defaultConnectorLabel>
        <rules>
            <name>outMetadataComponentChangesYes</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>varCountMetadataComponentChanges</leftValueReference>
                <operator>GreaterThan</operator>
                <rightValue>
                    <numberValue>0.0</numberValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>loopOnChanges</targetReference>
            </connector>
            <label>Yes</label>
        </rules>
    </decisions>
    <decisions>
        <description>Only a Builder or Owner/Lead can transfer stories to a release.</description>
        <name>decIsCurrentUserAuthorized</name>
        <label>Is Current User Authorized?</label>
        <locationX>1568</locationX>
        <locationY>350</locationY>
        <defaultConnectorLabel>Not Authorized</defaultConnectorLabel>
        <rules>
            <name>outCurrentUserAuthorizedYES</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>subflowCheckRole.outputIsAuthorized</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>getReleases</targetReference>
            </connector>
            <label>Authorized</label>
        </rules>
    </decisions>
    <decisions>
        <name>decStoryFound</name>
        <label>Story Found?</label>
        <locationX>314</locationX>
        <locationY>890</locationY>
        <defaultConnector>
            <targetReference>setNewPriorityTo1</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>No</defaultConnectorLabel>
        <rules>
            <name>outStoryFoundYes</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>getLowestPriorityStory.Id</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>setNewPriorityToNPlus1</targetReference>
            </connector>
            <label>Yes</label>
        </rules>
    </decisions>
    <description>Moves the selected story from one release (like a backlog) to another.</description>
    <environments>Default</environments>
    <formulas>
        <name>formulaNewStoryPriority</name>
        <dataType>Number</dataType>
        <expression>{!getLowestPriorityStory.Priority_Order__c}+1</expression>
        <scale>0</scale>
    </formulas>
    <interviewLabel>Imhotep - Story - Transfer {!$Flow.CurrentDateTime}</interviewLabel>
    <isAdditionalPermissionRequiredToRun>true</isAdditionalPermissionRequiredToRun>
    <isTemplate>true</isTemplate>
    <label>Imhotep - Story - Transfer</label>
    <loops>
        <name>loopOnChanges</name>
        <label>Loop on Changes</label>
        <locationX>50</locationX>
        <locationY>1622</locationY>
        <collectionReference>getMetadataComponentChanges</collectionReference>
        <iterationOrder>Asc</iterationOrder>
        <nextValueConnector>
            <targetReference>setUpdateChange</targetReference>
        </nextValueConnector>
        <noMoreValuesConnector>
            <targetReference>updateChanges</targetReference>
        </noMoreValuesConnector>
    </loops>
    <processMetadataValues>
        <name>BuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>CanvasMode</name>
        <value>
            <stringValue>AUTO_LAYOUT_CANVAS</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>OriginBuilderType</name>
        <value>
